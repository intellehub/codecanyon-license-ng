{
  "name": "shah-newaz/codecanyon-license-ng",
  "description": "CodeCanyon License Checker for Laravel Apps",
  "keywords": ["laravel", "codecanyon", "license"],
  "license": "MIT",
  "authors": [
    {
      "name": "Shahnewaz Rifat",
      "email": "rifat_rifats@live.com"
    }
  ],
  "require": {
    "laravel/framework": "^8.0",
    "php": "^7.3",
    "vlucas/phpdotenv": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
        "Shahnewaz\\CodeCanyonLicensorNg\\": "src/"
    },
    "files": [
        "src/helpers/general.php"
    ]
  },
	"extra": {
    "laravel": {
      "providers": [
          "Shahnewaz\\CodeCanyonLicensorNg\\Providers\\LicenseServiceProvider"
      ],
      "aliases": {
        "Licensor": "Shahnewaz\\CodeCanyonLicensorNg\\Facades\\Licensor"
      }
    }
	}
}
